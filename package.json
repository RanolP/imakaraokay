{
  "name": "imakaraokay",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm -F website dev",
    "build": "pnpm -F website build",
    "preview": "pnpm -F website preview",
    "build:all": "tsdown && pnpm -F website build",
    "build:libs": "tsdown",
    "dev:libs": "tsdown --watch",
