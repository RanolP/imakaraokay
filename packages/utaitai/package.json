{
  "name": "@imakaraokay/utaitai",
  "version": "1.0.0",
  "description": "Utaitai library for imakaraokay",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "type-check": "tsc --noEmit",
    "lint": "biome check src/",
    "lint:fix": "biome check --write src/",
    "format": "biome format src/",
    "format:fix": "biome format --write src/",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "dependencies": {
    "cheerio": "^1.1.0",
    "ky": "^1.8.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.6",
    "@vitest/ui": "^2.1.6"
  },
  "keywords": [
    "karaoke",
    "utaitai",
    "library"
  ],
  "author": "RanolP",
  "license": "MIT"
}
